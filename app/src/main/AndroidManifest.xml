<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arthenica.mysongapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".audio.audioedit.App"
        android:theme="@style/AppTheme">
        <activity android:name=".ffmpegexport.FfmpegActivity"></activity>
        <activity android:name=".dialogfragment.DialogFragmentActivity" />
        <activity android:name=".example.ExampleActivity" />
        <activity android:name=".playerandvoice.ExoplayerWithAudioActivity" />
        <activity android:name=".chooseaudio.ChooseAudioActivity" />
        <activity android:name=".consumeScrollview.ConsumerScrollActivity" />
        <activity android:name=".recyclemvvm.RecycleMvvmActivity" />
        <activity android:name=".audio.AudioActivity" />
        <activity android:name=".recycledemo.LoadMoreWrapperActivity" />
        <activity android:name=".imagecut.ImageActivity" />

        <service
            android:name=".UploadFileService"
            android:exported="false" />
        <service
            android:name=".exportservice.ExportVideoService"
            android:enabled="true"
            android:exported="false"
            android:process=":exportService" />
        <service
            android:name=".SongService"
            android:enabled="true"
            android:exported="false"
            android:process=".song" />

        <activity android:name=".progress.ProgressActivity" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="sensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".audio.audioedit.AudioEditActivity"/>
        <service android:name=".audio.audioedit.service.AudioTaskService"/>
        <activity android:name=".audio.audioedit.MainActivity2"/>
    </application>

</manifest>